{
  "multipart": [
    {
      "apply": {
        "model": "rtdd:block/d_39_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "rtdd:block/d_39_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "rtdd:block/d_39_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "rtdd:block/d_39_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "rtdd:block/d_39_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "rtdd:block/d_39_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "rtdd:block/d_39_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "rtdd:block/d_39_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "rtdd:block/d_39_noside",
        "y": 270
      }
    }
  ]
}